{
  "name": "auto-id-class",
  "main": "./lib/auto-id-class",
  "version": "0.1.0",
  "description": "Emulate Sublime Text's `auto_id_class` setting allowing shortcuts of `.` and `#` in HTML tags to create ` class=\"\"` and ` id=\"\"` respectively.",
  "keywords": [
      "auto_id_class",
      "auto_class_id",
      "sublime auto id class"
  ],
  "activationCommands": {
    "atom-editor": "auto-id-class:auto_id",
    "atom-editor": "auto-id-class:auto_class"
  },
  "repository": "https://github.com/bradleyflood/auto-id-class",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
  }
}
